FUNCTION factorial:
  PARAM n
  t1 = n
  t2 = #0
  IF t1 == t2 GOTO Label1
  GOTO Label2
LABEL Label1:
  t3 = #1
  RETURN t3
  GOTO Label3
LABEL Label2:
  t5 = n
  t8 = n
  t9 = #1
  t7 = t8 - t9
  ARG t7
  t6 = CALL factorial
  t4 = t5 * t6
  RETURN t4
LABEL Label3:
FUNCTION main:
  n = CALL read
  t10 = n
  ARG t10
  result = CALL factorial
  t12 = result
  ARG t12
  t11 = CALL write
  t13 = #0
  RETURN t13
