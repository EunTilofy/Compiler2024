FUNCTION main:
    n = CALL read
    t1 = n
    t2 = #0
    IF t1 > t2 GOTO Label1
    GOTO Label2
    LABEL Label1:
    t4 = #1
    ARG t4
    t3 = CALL write
    GOTO Label3
    LABEL Label2:
    t5 = n
    t6 = #0
    IF t5 < t6 GOTO Label4
    GOTO Label5
    LABEL Label4:
    t9 = #1
    t10 = #0
    t8 = t10 - t9
    ARG t8
    t7 = CALL write
    GOTO Label6
    LABEL Label5:
    t12 = #0
    ARG t12
    t11 = CALL write
    LABEL Label6:
    LABEL Label3:
    t13 = #0
    RETURN t13